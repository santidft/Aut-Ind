<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="inicializar">
    <Code><![CDATA[begin
  
  // Booleanos
  bPiezaPreparada=false
  bFin=false
  bTermina=false
  bStartPiramide=false

  // Variables
  // Longitud
  nLPieza=69
  // Ancho
  nWPieza=23
  // Altura
  nHPieza=12

  nLMesa=400
  nWMesa=300
  
  nPlantas=0
  nNivel=0
  nFila=0
  nCont=0
  nX=0
  nY=0
  nZ=0
  
  sMensaje=""
  
  // Sistema de referencia
  pCentro.trsf.x=pPieza.trsf.x-(nLMesa/2-nLPieza/2)
  pCentro.trsf.y=pPieza.trsf.y+(nWMesa/2-nWPieza/2)
  pCentro.trsf.z=pPieza.trsf.z

end]]></Code>
  </Program>
</Programs>