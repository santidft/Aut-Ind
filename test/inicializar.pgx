<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="inicializar">
    <Code><![CDATA[begin
  
  // Booleanos
  bPiezaPreparada=false
  bFin=false
  bTermina=false
  bStartPiramide=false

  // Variables Pieza
  // Longitud
  nLPieza=71
  // Ancho
  nWPieza=25
  // Altura
  nHPieza=12

  // Variables mesa de trabajo
  nLMesa=400 
  nWMesa=300 
  
  // Variables y contadores usados
  // Plantas introducidas por usuario
  nPlantas=0 
  // Planta actual
  nNivel=0 
  // Fila actual
  nFila=0 
  // Contador para variacion de fila
  nCont=0 
  // Variacion en x (fila)
  nX=0 
  // Variacion en y (pieza de la fila)
  nY=0 
  // Variacion en z (nivel). Valor = margen de error
  nZ=1 
  
  //Mensaje a mostrar en pantalla
  sMensaje="" 
  
  // Sistema de referencia
  pCentro=appro(pPieza,{(nWMesa/2-nWPieza/2),-(nLMesa/2-nLPieza/2),0,0,0,0})

end]]></Code>
  </Program>
</Programs>