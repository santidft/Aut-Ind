<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="inicializar">
    <Code><![CDATA[begin
  
  // Booleanos
  bPiezaPreparada=false
  bFin=false
  bTermina=false
  bStartPiramide=false

  // Variables Pieza
  // Longitud
  nLPieza=71
  // Ancho
  nWPieza=25
  // Altura
  nHPieza=12

  // Variables mesa de trabajo
  nLMesa=400 
  nWMesa=300 
  
  // Variables y contadores usados
  nPlantas=0 // Plantas introducidas por usuario
  nNivel=0 // Planta actual
  nFila=0 // Fila actual
  nCont=0 // Contador para variación de fila
  nX=0 // Variación en x (fila)
  nY=0 // Variación en y (pieza de la fila)
  nZ=2 // Variación en z (nivel). Valor = margen de error
  
  sMensaje="" //Mensaje a mostrar en pantalla
  
  // Sistema de referencia
  pCentro=appro(pPieza,{(nWMesa/2-nWPieza/2),-(nLMesa/2-nLPieza/2),0,0,0,0})

end]]></Code>
  </Program>
</Programs>